<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
						"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.innovaee.eorder.web.action.resetpassword" namespace="/resetpassword" extends="struts-base">
		<action name="doResetPassword" class="com.innovaee.eorder.web.action.resetpassword.ResetPasswordAction" method="doResetPassword">
            <result name="success">/pages/resetpassword/resetpassword.jsp</result>
        </action>
        
        <action name="doReset" class="com.innovaee.eorder.web.action.resetpassword.ResetPasswordAction" method="doReset">
            <result name="success">/pages/resetpassword/resetpassword.jsp</result>
        </action>
        
        <action name="main" class="com.innovaee.eorder.web.action.resetpassword.ResetPasswordAction" method="login">
            <result name="success">/pages/resetpassword/resetpassword.jsp</result>
        </action>
    </package>

</struts>
