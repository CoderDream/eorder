<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
						"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.innovaee.eorder.web.action.admin.userrole"
		namespace="/userrole" extends="struts-base">
		<action name="doUserRole"
			class="com.innovaee.eorder.web.action.admin.userrole.UserRoleAction" method="doUserRole">
			<result name="success">/pages/admin/userrole/userrole.jsp</result>
		</action>

		<action name="doList"
			class="com.innovaee.eorder.web.action.admin.userrole.UserRoleAction" method="list">
			<result>/pages/admin/userrole/userrole.jsp</result>
		</action>
		<action name="doLoad"
			class="com.innovaee.eorder.web.action.admin.userrole.UserRoleAction" method="load">
			<result>/pages/admin/userrole/userrole.jsp</result>
		</action>
		<action name="doStore"
			class="com.innovaee.eorder.web.action.admin.userrole.UserRoleAction" method="doStore">
			<result type="redirect">doRoleFunction.action</result>
		</action>
		<action name="doUpdate"
			class="com.innovaee.eorder.web.action.admin.userrole.UserRoleAction" method="doUpdate">
			<result>/pages/admin/userrole/userrole.jsp</result>
		</action>
	</package>

</struts>